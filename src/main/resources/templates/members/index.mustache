
{{>layouts/header}}
<h1> 회원 목록 </h1>

<table class="table">
    <thead>
    <tr>
        <th scope="col">번호</th>
        <th scope="col">아이디</th>
        <th scope="col">이름</th>
        <th scope="col">이메일</th>
    </tr>
    </thead>
    <tbody>
    {{#memberList}}
        <tr>
            <th scope="row">{{id}}</th>
            <td><a href="/members/{{id}}">{{memberId}}</a></td>
            <td>{{name}}</td>
            <td>{{email}}</td>
        </tr>
    {{/memberList}}
    </tbody>
</table>

<a href="/members/new" class="btn btn-primary">회원 가입</a>

{{#msg}}
    <script>
        alert("{{msg}}");
    </script>
{{/msg}}


{{>layouts/footer}}